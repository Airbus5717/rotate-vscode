{
    "name": "rotate-vscode",
    "displayName": "Rotate",
    "description": "Syntax highlighter for rotate programming language",
    "repository": {
        "url":"https://github.com/rotate-lang/rotate-vscode"
    },
    "publisher": "Airbus5717",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "rotate",
                "aliases": [
                    "Rotate"
                ],
                "extensions": [
                    ".vr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rotate",
                "scopeName": "source.vr",
                "path": "./syntaxes/rotate.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "rotate",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
